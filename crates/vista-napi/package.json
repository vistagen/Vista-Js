{
  "name": "vista-native",
  "version": "0.1.0",
  "description": "Native Rust bindings for Vista framework",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "vista-native",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "x86_64-apple-darwin",
        "x86_64-unknown-linux-gnu",
        "aarch64-unknown-linux-gnu",
        "x86_64-pc-windows-msvc"
      ]
    }
  },
  "scripts": {
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.4"
  }
}
