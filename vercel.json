{
  "version": 2,
  "buildCommand": "cd packages/vista && npm install --legacy-peer-deps && cd ../../apps/web && npm install --legacy-peer-deps && npx vista build",
  "outputDirectory": "apps/web/.vista",
  "framework": null,
  "installCommand": "npm install --legacy-peer-deps",
  "devCommand": "cd apps/web && npm run dev",
  "routes": [
    {
      "src": "/client.js",
      "dest": "/apps/web/.vista/client.js"
    },
    {
      "src": "/client.css",
      "dest": "/apps/web/.vista/client.css"
    },
    {
      "src": "/vista-client-manifest.js",
      "dest": "/apps/web/.vista/vista-client-manifest.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/handler"
    }
  ]
}
